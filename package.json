{
  "name": "egov-law-mcp",
  "version": "0.1.0",
  "description": "MCP Server for Japan e-Gov Law API - Access Japanese laws and regulations",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "bin": {
    "egov-law-mcp": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc && chmod 755 dist/index.js",
    "prepare": "npm run build",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "test": "node --test tests/*.test.js",
    "typecheck": "tsc --noEmit",
    "inspector": "npx @modelcontextprotocol/inspector dist/index.js"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "egov",
    "japan",
    "law",
    "legal",
    "api"
  ],
  "author": "ymadd",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ymadd/egov-law-mcp.git"
  },
  "homepage": "https://github.com/ymadd/egov-law-mcp#readme",
  "bugs": {
    "url": "https://github.com/ymadd/egov-law-mcp/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "fast-xml-parser": "^4.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0"
  },
  "files": [
    "dist"
  ]
}
